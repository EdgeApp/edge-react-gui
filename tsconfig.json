{
  "compilerOptions": {
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": [
      "es2017"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "esnext",
    "types": [
      "react-native"
    ],
    "strict": true
  }
}