<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="splash"
            android:width="432dp"
            android:height="432dp"
            android:viewportWidth="432"
            android:viewportHeight="432">
            <group
                android:name="group"
                android:translateX="100"
                android:translateY="100"
                android:scaleX="0.4"
                android:scaleY="0.4">
                <path
                    android:name="body"
                    android:pathData="M 538.3 115.7 L 388.6 526 L 41.9 526 L 119.8 312.7 L 257.3 312.7 L 329.1 115.7 Z"
                    android:fillColor="#00f1a2"
                    android:strokeWidth="1"/>
                <group
                    android:name="corner_g"
                    android:translateX="80"
                    android:translateY="70">
                    <path
                        android:name="corner"
                        android:pathData="M 298.3 46 L 218.3 261.4 L 33.7 261.4 L 111.6 46 Z"
                        android:fillColor="#00f1a2"
                        android:strokeWidth="1"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0.4"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="500"
                    android:valueFrom="0"
                    android:valueTo="0.4"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateX"
                    android:duration="500"
                    android:valueFrom="200"
                    android:valueTo="100"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:duration="500"
                    android:valueFrom="200"
                    android:valueTo="100"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="corner_g">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateX"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="80"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="500"
                    android:duration="500"
                    android:valueFrom="70"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_decelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
