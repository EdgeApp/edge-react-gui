    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-native": "^3.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "flow-bin": "^0.75.0",
    "flow-coverage-report": "^0.4.1",
