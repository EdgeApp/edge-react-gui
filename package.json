{
  "name": "edge",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "updot": "dotdotdep",
    "android:run": "yarn run android:clean; react-native run-android",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix --verbose | snazzy",
    "android:clean": "cd android && ./gradlew clean && rm -rf build && cd ../",
    "android:release": "cd android && ./gradlew assembleRelease; cd ../",
    "android:release-install": "yarn run android:clean && yarn run android:release && cd android/app/build/outputs/apk && adb install app-release.apk; cd ../",
    "android:logcat": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "ios:run": "react-native run-ios",
    "test": "NODE_ENV=production ava",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "tron": "node_modules/.bin/reactotron",
    "flow": "flow --show-all-errors"
  },
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  },
